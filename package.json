{
  "name": "h5p-highlight-the-words",
  "version": "0.1.4",
  "description": "Highlight words and sort them into the correct category",
  "private": true,
  "scripts": {
    "test": "jest --silent",
    "build": "cross-env NODE_ENV=production ./node_modules/.bin/webpack",
    "watch": "./node_modules/.bin/webpack --watch"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/src/stylesheets/"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/src/stylesheets/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/otacke/h5p-highlight-the-words.git"
  },
  "keywords": [
    "h5p",
    "library"
  ],
  "author": "Oliver Tacke",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-destructuring": "^7.12.13",
    "@babel/preset-env": "^7.12.10",
    "babel-loader": "^8.2.2",
    "cross-env": "^5.2.1",
    "css-loader": "^5.0.1",
    "eslint": "^7.18.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.4",
    "regenerator-runtime": "^0.13.7",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.0.0"
  },
  "dependencies": {
    "he": "^1.2.0"
  }
}
